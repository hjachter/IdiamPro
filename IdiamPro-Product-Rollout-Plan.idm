    "legal3": {
      "id": "legal3",
      "name": "Privacy Policy",
      "content": "<p>Must comply with:</p><p>• GDPR (EU users)</p><p>• CCPA (California users)</p><p>• Apple App Store requirements</p><p></p><p>Cover:</p><p>• What data is collected</p><p>• How data is used/stored</p><p>• User rights (access, deletion)</p><p>• Third-party services (analytics, payments)</p><p></p><p><strong>Tools:</strong> Termly, iubenda (auto-generate + updates)</p>",
      "type": "task",
      "parentId": "phase2",
      "childrenIds": [],
      "isCollapsed": false,
