    "payment1": {
      "id": "payment1",
      "name": "Define Pricing Strategy",
      "content": "<p><strong>Recommended Model:</strong> Freemium + Premium Tiers</p><p></p><p><strong>FREE Plan:</strong></p><p>• 3 outlines max</p><p>• All features unlocked</p><p>• Local storage only</p><p></p><p><strong>PRO Plan ($7-10/month or $70-100/year):</strong></p><p>• Unlimited outlines</p><p>• Cloud sync (optional)</p><p>• Priority support</p><p>• Early access to new features</p><p></p><p><strong>TEAM Plan ($15-20/user/month):</strong></p><p>• Shared outlines</p><p>• Collaboration features</p><p>• Admin controls</p>",
      "type": "document",
      "parentId": "phase3",
      "childrenIds": [],
      "isCollapsed": false,
      "prefix": ""
    },
    "payment2": {
      "id": "payment2",
      "name": "Stripe Integration",
      "content": "<p><strong>Setup:</strong></p><p>• Create Stripe account</p><p>• Complete business verification</p><p>• Set up products and pricing</p><p>• Configure webhooks</p><p></p><p><strong>Implement:</strong></p><p>• Checkout flow (Stripe Checkout or Elements)</p><p>• Subscription management</p><p>• Customer portal (update card, cancel)</p><p>• Invoice emails</p><p></p><p><strong>Test:</strong></p><p>• Test mode transactions</p><p>• Refund flow</p><p>• Failed payment handling</p>",
      "type": "task",
      "parentId": "phase3",
