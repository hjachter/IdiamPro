    "22ecf66f-14ce-42b5-9eaf-b3920fbb5b27": {
      "id": "22ecf66f-14ce-42b5-9eaf-b3920fbb5b27",
      "name": "Pricing Structure",
      "content": "The pricing structure includes a Free tier ($0) with basic features and 1,000 API calls/month, a Premium tier ($19/mo or $15 annual) with AI features and 10,000 API calls/month, a Pro tier ($49/mo or $39 annual) with unlimited AI and 50,000 API calls/month, and an Enterprise tier (custom pricing) with team collaboration and custom API calls.",
      "type": "document",
      "parentId": "d56d2631-c1ae-44a4-b3e2-d1598978edf5",
      "childrenIds": [],
      "isCollapsed": false,
      "prefix": "1.4.2"
