    "wf-content": {
      "id": "wf-content",
      "name": "Content Creation & Writing",
      "content": "<p>Workflows for writers, authors, and content creators.</p>",
      "type": "document",
      "parentId": "workflows",
      "childrenIds": [
        "wf-1",
        "wf-2",
        "wf-3",
        "wf-4",
        "wf-5"
      ],
      "isCollapsed": true,
