    "platforms-current": {
      "id": "platforms-current",
      "name": "Currently Available",
      "content": "<h3>macOS (Desktop App)</h3><ul><li><p><strong>Technology:</strong> Electron app, distributed as a .dmg installer</p></li><li><p><strong>Architecture:</strong> Universal binary — works on both Apple Silicon and Intel Macs</p></li><li><p><strong>Features:</strong> Full feature set including Ollama integration (private AI) and native file system access</p></li><li><p><strong>Minimum OS:</strong> macOS 11 (Big Sur) or later</p></li></ul><h3>iOS / iPadOS (Mobile)</h3><ul><li><p><strong>Technology:</strong> Native app built with Capacitor, distributed via the App Store</p></li><li><p><strong>Features:</strong> Full feature set with touch-optimized gestures and native share sheet</p></li><li><p><strong>Minimum OS:</strong> iOS 15 or later</p></li></ul><h3>Web Browser</h3><ul><li><p><strong>Supported:</strong> Chrome, Edge, Safari, and Firefox</p></li><li><p>No installation needed — just open in your browser</p></li><li><p>Chrome and Edge provide the best experience thanks to the File System Access API, which lets you save files directly to your computer</p></li></ul><p></p>",
      "type": "document",
      "parentId": "platforms",
      "childrenIds": [],
      "isCollapsed": false,
      "prefix": "2.1"
    },
    "platforms-future": {
      "id": "platforms-future",
      "name": "Future Platforms (Roadmap)",
      "content": "<h3>Coming Soon</h3><ul><li><p><strong>Windows:</strong> Electron app with NSIS installer + portable version</p></li><li><p><strong>Linux:</strong> Available as AppImage and DEB packages</p></li><li><p><strong>Android:</strong> Built with Capacitor, distributed via Google Play Store</p></li></ul><h3>Under Consideration</h3><ul><li><p><strong>Chrome Extension:</strong> Quick capture — grab ideas from any web page</p></li><li><p><strong>VS Code Extension:</strong> Integration for software developers</p></li><li><p><strong>Obsidian Plugin:</strong> Connect with the popular knowledge management app</p></li></ul><p></p>",
      "type": "document",
      "parentId": "platforms",
      "childrenIds": [],
      "isCollapsed": false,
      "prefix": "2.2"
    },
    "platforms-comparison": {
      "id": "platforms-comparison",
      "name": "Platform Feature Comparison",
      "content": "<h3>What Works Where</h3><p><strong>Available everywhere</strong> (Mac, iPhone/iPad, Web, and coming to Windows and Linux):</p><ul><li><p>All outlining features</p></li><li><p>AI content generation</p></li><li><p>PDF, YouTube, and web page import</p></li><li><p>Audio transcription</p></li><li><p>Keyboard shortcuts (partial on iPhone/iPad)</p></li></ul><p><strong>Desktop only</strong> (Mac, and coming to Windows and Linux):</p><ul><li><p>Private AI (runs AI on your own computer, no internet needed)</p></li><li><p>Full file system access (save anywhere on your computer)</p></li></ul><p><strong>iPhone and iPad only:</strong></p><ul><li><p>Touch gestures (swipe to indent/outdent, drag to reorganize)</p></li></ul><p><em>Windows and Linux versions coming soon</em></p>",
      "type": "document",
      "parentId": "platforms",
      "childrenIds": [],
      "isCollapsed": false,
      "prefix": "2.3"
    },
