    "req-macos": {
      "id": "req-macos",
      "name": "macOS Requirements",
      "content": "<h3>Minimum Requirements</h3><ul><li><p><strong>OS:</strong> macOS 11.0 (Big Sur) or later</p></li><li><p><strong>Processor:</strong> Intel Core i5 or Apple M1</p></li><li><p><strong>RAM:</strong> 4 GB</p></li><li><p><strong>Storage:</strong> 500 MB for application</p></li><li><p><strong>Internet:</strong> Required for AI features (unless using private AI)</p></li></ul><h3>Recommended</h3><ul><li><p><strong>OS:</strong> macOS 13.0 (Ventura) or later</p></li><li><p><strong>Processor:</strong> Apple M1 or later</p></li><li><p><strong>RAM:</strong> 8 GB+ (16 GB for large outlines)</p></li><li><p><strong>Storage:</strong> SSD with 2 GB+ free space</p></li></ul><p></p>",
      "type": "document",
      "parentId": "requirements",
      "childrenIds": [],
      "isCollapsed": false,
      "prefix": "3.1"
    },
    "req-ios": {
      "id": "req-ios",
      "name": "iOS Requirements",
      "content": "<h3>Minimum Requirements</h3><ul><li><p><strong>OS:</strong> iOS 15.0 or later</p></li><li><p><strong>Device:</strong> iPhone 8 or later, iPad (6th gen) or later</p></li><li><p><strong>Storage:</strong> 200 MB for application</p></li><li><p><strong>Internet:</strong> Required for AI features</p></li></ul><h3>Recommended</h3><ul><li><p><strong>OS:</strong> iOS 17.0 or later</p></li><li><p><strong>Device:</strong> iPhone 12 or later, iPad Pro</p></li><li><p><strong>Storage:</strong> 1 GB+ free for outline storage</p></li></ul><p></p>",
      "type": "document",
      "parentId": "requirements",
      "childrenIds": [],
      "isCollapsed": false,
      "prefix": "3.2"
    },
    "req-web": {
      "id": "req-web",
      "name": "Web Browser Requirements",
      "content": "<h3>Supported Browsers</h3><ul><li><p><strong>Chrome:</strong> Version 100 or later (recommended)</p></li><li><p><strong>Edge:</strong> Version 100 or later</p></li><li><p><strong>Safari:</strong> Version 15 or later</p></li><li><p><strong>Firefox:</strong> Version 100 or later</p></li></ul><h3>Notes</h3><ul><li><p>Chrome and Edge offer the best experience — they let you save files directly to your computer</p></li><li><p>Your browser must allow cookies and local storage so your work is saved</p></li></ul><p></p>",
      "type": "document",
      "parentId": "requirements",
      "childrenIds": [],
      "isCollapsed": false,
      "prefix": "3.3"
    },
    "req-optional": {
      "id": "req-optional",
      "name": "Optional Components",
      "content": "<h3>Ollama — Private, Offline AI (Optional)</h3><p>Ollama lets you run AI entirely on your own computer — nothing is sent to the cloud. Install it and IdiamPro connects automatically.</p><ul><li><p><strong>Download:</strong> <a target=\"_blank\" rel=\"noopener noreferrer nofollow\" href=\"https://ollama.ai\">ollama.ai</a></p></li><li><p><strong>RAM:</strong> 8 GB minimum (16 GB recommended)</p></li><li><p><strong>Storage:</strong> 4–8 GB per model</p></li><li><p><strong>Recommended models:</strong></p><ul><li><p><code>llama3.2</code> — Best balance of speed and quality (default)</p></li><li><p><code>phi3</code> — Fastest responses, lighter weight</p></li><li><p><code>llama3.1</code> — Highest quality output</p></li><li><p><code>codellama</code> — Optimized for code-related tasks</p></li></ul></li></ul><h3>AssemblyAI — Audio Transcription (Optional)</h3><p>Converts audio and video recordings to text, with speaker diarization (identifies who said what).</p><ul><li><p>Sign up at <a target=\"_blank\" rel=\"noopener noreferrer nofollow\" href=\"https://www.assemblyai.com\">assemblyai.com</a> and add your API key to IdiamPro's settings</p></li><li><p>Free tier: 100 hours/month for testing</p></li></ul><p></p>",
      "type": "document",
      "parentId": "requirements",
      "childrenIds": [],
      "isCollapsed": false,
      "prefix": "3.4"
    },
    "features": {
      "id": "features",
      "name": "Features & Capabilities",
      "content": "<h2>Complete Feature Set</h2><p>IdiamPro combines powerful outlining with AI-native capabilities.</p>",
      "type": "chapter",
      "parentId": "root",
      "childrenIds": [
        "feat-outlining",
