      "id": "subscriptions",
      "name": "Subscription Tiers",
      "content": "<h2>Pricing Plans</h2><p>Choose the plan that fits your needs.</p>",
      "type": "chapter",
      "parentId": "root",
      "childrenIds": [
        "sub-free",
        "sub-premium"
      ],
      "isCollapsed": true,
      "prefix": "7"
