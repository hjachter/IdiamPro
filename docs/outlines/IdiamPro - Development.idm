    "55b7c505-c1bb-4c96-823a-71dea3b8a6a7": {
      "id": "55b7c505-c1bb-4c96-823a-71dea3b8a6a7",
      "name": "Supported Platforms",
      "content": "<p><strong>macOS</strong></p><p>&nbsp; - .dmg installer (universal: Intel + Apple Silicon)</p><p>&nbsp; - .zip for auto-updates</p><p></p><p>&nbsp; <strong>Windows</strong></p><p>&nbsp; - .exe NSIS installer (x64 + ARM64)</p><p>&nbsp; - Portable .exe (x64 + ARM64)</p><p></p><p>&nbsp; <strong>Linux</strong></p><p>&nbsp; - .AppImage (x64 + ARM64)</p><p>&nbsp; - .deb Debian/Ubuntu package (x64 + ARM64)</p><p></p><p>&nbsp; <strong>Web</strong></p><p>&nbsp; - Webapp via Vercel (any browser)</p><p></p><p>&nbsp; <strong>Mobile</strong> (configured but not built yet)</p><p>&nbsp; - iOS via Capacitor (requires Xcode)</p><p>&nbsp; - Android via Capacitor (requires Android Studio)</p>",
