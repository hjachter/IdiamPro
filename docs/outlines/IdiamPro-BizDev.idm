    "bp-vision": {
      "id": "bp-vision",
      "name": "Vision Statement",
      "content": "<h3>Vision</h3><p><strong>\"Democratize knowledge synthesis and accelerate human creativity with AI.\"</strong></p><p>We envision a world where anyone can synthesize complex information from hundreds of sources and transform it into publication-ready work in hours instead of months.</p><h3>Long-Term Vision (5 Years)</h3><ul><li>Every researcher uses IdiamPro to write papers and theses</li><li>Authors write and publish books 10x faster</li><li>Journalists synthesize investigations in days</li><li>Students learn by creating comprehensive knowledge maps</li><li>IdiamPro becomes the standard for knowledge work</li></ul>",
      "type": "document",
      "parentId": "business-plan",
      "childrenIds": [],
      "prefix": "2.1"
    },
    "bp-mission": {
      "id": "bp-mission",
      "name": "Mission Statement",
      "content": "<h3>Mission</h3><p><strong>\"Build the world's most powerful AI-assisted research synthesis and content creation platform.\"</strong></p><p>We empower researchers, academics, and authors to:</p><ol><li>Import and analyze unlimited sources with AI</li><li>Organize knowledge into structured outlines</li><li>Generate high-quality content automatically</li><li>Export publication-ready documents</li><li>Collaborate and share knowledge</li></ol>",
      "type": "document",
      "parentId": "business-plan",
      "childrenIds": [],
      "prefix": "2.2"
    },
    "bp-values": {
      "id": "bp-values",
      "name": "Core Values",
      "content": "<h3>Core Values</h3><ol><li><strong>Quality Over Speed:</strong> AI-generated content must be research-grade</li><li><strong>User Privacy:</strong> User data is sacred, never sold or compromised</li><li><strong>Transparency:</strong> Clear about AI capabilities and limitations</li><li><strong>Academic Integrity:</strong> Tools to support proper attribution and citations</li><li><strong>Continuous Innovation:</strong> Integrate best AI models as they emerge</li></ol>",
      "type": "document",
      "parentId": "business-plan",
      "childrenIds": [],
      "prefix": "2.3"
